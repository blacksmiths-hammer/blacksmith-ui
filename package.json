{
  "name": "@hesiod3c/ui",
  "version": "0.4.5",
  "description": "Descco UI",
  "scripts": {
    "build:react": "cross-env NODE_ENV=production babel-node bin/build/build-cli.js && webpack --progress --config ./internals/webpack/react/webpack.config.js",
    "build:storybook": "cross-env NODE_ENV=development build-storybook -c ./internals/stories-config/storybook -o .out",
    "lint": "yarn lint:sass && yarn lint:js",
    "lint:js": "eslint shared/react/**",
    "lint:sass": "sass-lint 'shared/scss/**/*.scss' --verbose --no-exit",
    "precommit": "yarn lint",
    "prepublish": "yarn build",
    "prepush": "jest --setupTestFrameworkScriptFile=internals/test/setup.js",
    "storybook": "start-storybook -p 9000 -c ./internals/storybook",
    "test": "jest --setupTestFrameworkScriptFile=internals/test/setup.js",
    "test:storybook": "storyshots -c ./internals/stories-config/storybook",
    "play": "vbuild -c play.config.js -d",
    "build:play": "vbuild -c play.config.js"
  },
  "main": "lib/index.js",
  "keywords": [
    "descco",
    "theme"
  ],
  "author": "Hemerson Vianna <hemerson.lourenco@gmail.com> (https://nerdcalistenico.com.br)",
  "peerDependencies": {
    "prop-types": ">=15.5.10",
    "react": ">=15.6.1",
    "react-dom": ">=15.6.1"
  },
  "dependencies": {
    "babel-loader": "8.0.6",
    "classnames": "2.2.6",
    "immutable": "4.0.0-rc.12",
    "moment": "2.24.0",
    "node-sass": "4.13.1",
    "paginator": "1.0.0",
    "prop-types": "15.7.2",
    "react-css-modules": "4.7.11",
    "react-onclickoutside": "6.8.0",
    "react-redux": "7.1.0",
    "react-transition-group": "4.2.1",
    "redux": "4.0.4",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "svg-react-loader": "0.4.6"
  },
  "devDependencies": {
    "@storybook/addon-events": "5.1.9",
    "@storybook/addon-info": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-options": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@storybook/ui": "5.1.9",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-css-modules-transform": "1.6.2",
    "babel-plugin-webpack-loaders": "0.9.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "chai": "4.2.0",
    "child-process-es6-promise": "1.2.1",
    "colors": "1.3.3",
    "create-react-class": "15.6.3",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-loader": "2.2.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "4.0.0",
    "fs-extra": "8.1.0",
    "fs-promise": "2.0.3",
    "husky": "3.0.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "0.8.0",
    "jest-cli": "24.8.0",
    "postcss-cssnext": "3.1.0",
    "postcss-loader": "3.0.0",
    "precss": "4.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-test-renderer": "16.8.6",
    "rimraf": "2.6.3",
    "sass-lint": "1.13.1",
    "sass-loader": "7.1.0",
    "sinon": "7.3.2",
    "sinon-chai": "3.3.0",
    "style-loader": "0.23.1",
    "url-loader": "2.0.1",
    "vbuild": "7.2.4",
    "vue": "2.6.10",
    "vue-play": "3.2.1",
    "vue-play-cli": "1.1.1",
    "vuex": "3.1.1",
    "webpack": "4.35.3",
    "webpack-dev-server": "3.7.2",
    "yargs": "13.2.4"
  },
  "engines": {
    "node": ">=6.10.3",
    "npm": ">=3.10.10"
  },
  "jest": {
    "verbose": false,
    "testRegex": "(/shared/.*.(test|spec)).(js|jsx)$",
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(s?css)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./internals/file-transformer.js"
    },
    "moduleDirectories": [
      "node_modules",
      "shared"
    ]
  }
}
